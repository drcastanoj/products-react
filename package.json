{
  "name": "team-international",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "json-server": "^0.15.0",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-md": "^1.12.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.2",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "node tools/apiServer.js",
    "start:dev": "run-p start:development start:api",
    "start:development": "react-scripts start",
    "clean:build": "rimraf ./build && mkdir build",
    "test:ci": "react-scripts test --coverage --watchAll=false",
    "prebuild": "run-p clean:build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prestart:api": "node tools/createMockDb.js",
    "start:api": "node tools/apiServer.js",
    "test-e2e": "cypress open",
    "cy": "run-p start:dev start:api test-e2e "
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.4",
    "cypress": "^3.3.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-enzyme": "^7.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!node_modules/"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
